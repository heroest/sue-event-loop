{
    "name": "sue/event-loop",
    "description": "提供基于reactphp的event-loop",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "heroest",
            "email": "kakashi@live.cn"
        }
    ],
    "autoload": {
        "psr-4": {
            "Sue\\EventLoop\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Sue\\EventLoop\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6.0",
        "react/event-loop": "^1.3",
        "react/promise": "^2.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3 || ^5.7"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    ]
}
